local player = game.Players.LocalPlayer.UserId
local t = {4997752964, 4913509869}
local isWhitelisted = false

for i, v in pairs(t) do
    if player == v then
        isWhitelisted = true
        break
    end
end

if not isWhitelisted then
    game.Players.LocalPlayer:Kick("Not whitelisted")
else
    print("Whitelisted")
end
